<Window x:Class="ITMO.CS_WPF.U1E1.WpfHello.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITMO.CS_WPF.U1E1.WpfHello"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="400">
    <Canvas>
        <Button x:Name="button" Content="Set Name" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,67,0,0" Click="Button_Click"/>
        <Button x:Name="button1" Content="Ret Name" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,103,0,0" Click="Button_Click_1" RenderTransformOrigin="1,0.7"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="1.983,3.609" Margin="116,64,0,0" TextChanged="textBox_TextChanged"/>
        <Label x:Name="label" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="3.5,2.5" Margin="116,97,0,0"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me" Height="23" Name="button3" Width="75" Click="button3_Click"/>
        <TextBox x:Name="myTextBox" Canvas.Left="90" Canvas.Top="12" Width="170"/>
    </Canvas>
</Window>
